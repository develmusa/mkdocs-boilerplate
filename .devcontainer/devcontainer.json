{
  "name": "Material for MkDocs",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [8000],
  "postCreateCommand": "mkdocs serve -a 0.0.0.0:8000",
  "features": {
    "ghcr.io/devcontainers/features/git:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "yzhang.markdown-all-in-one",
        "eamodio.gitlens",
        "mhutchie.git-graph"
      ]
    }
  }
}